{
  "name": "patchwork-boilerplate",
  "version": "0.0.1",
  "description": "A gulp + sass + vuejs boilerplate",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "http://github.com/ayamflow/patchwork"
  },
  "keywords": [
    "boilerplate",
    "vue.js",
    "gulp",
    "sass",
    "scss",
    "starter"
  ],
  "author": "= <=>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ayamflow/patchwork/issues"
  },
  "browserify": {
    "transform": [
      "html-browserify",
      "browserify-shim"
    ]
  },
  "browser": {
