{
  "name": "patchwork-boilerplate",
  "version": "0.2.0",
  "description": "Patchwork is a mix between a boilerplate and a miniframework built on top of Vue.js
It allows to quickly build prototypes or websites using gulp, sass, vue.js and GSAP.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "http://github.com/ayamflow/patchwork"
  },
  "keywords": [
    "boilerplate",
    "vue.js",
    "gulp",
    "sass",
    "scss",
    "starter"
  ],
  "author": "= <=>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ayamflow/patchwork/issues"
  },
  "browserify": {
    "transform": [
      "html-browserify",
      "browserify-shim"
    ]
  },
  "browser": {
    "TweenMax": "./node_modules/greensock/src/minified/TweenMax.min.js",
    "TweenMax.ScrollToPlugin": "./node_modules/greensock/src/minified/plugins/ScrollToPlugin.min.js",
    "fit": "./node_modules/fit/fit.min.js",
    "forEach": "./node_modules/foreach.js/dist/foreach.min.js"
  },
  "browserify-shim": {
    "TweenMax": "TweenMax",
    "fit": "fit",
    "forEach": "forEach"
  },
  "napa": {
    "greensock": "greensock/GreenSock-JS",
    "fit": "soulwire/fit.js",
    "foreach.js": "toddmotto/foreach"
  },
  "devDependencies": {
    "gulp": "*",
    "watchify": "*",
    "gulp-plumber": "*",
    "gulp-autoprefixer": "0.0.8",
    "gulp-sass": "*",
    "vinyl-source-stream": "*",
    "napa": "*",
    "browserify": "^5.9.1",
    "browserify-shim": "*",
    "gulp-notify": "^1.4.0",
    "vue-debug": "*",
    "html-browserify": "*",
    "del": "*",
    "gulp-jshint": "^1.8.2",
    "gulp-util": "^3.0.0",
    "gulp-uglify": "*",
    "pretty-hrtime": "*",
    "remapify": "*",
    "jshint-stylish": "*",
    "debounce": "*",
    "vue": "^0.10.6",
    "yargs": "*",
    "gulp-streamify": "0.0.5",
    "gulp-minify-css": "~0.3.8",
    "browser-sync": "~1.5.3"
  },
  "dependencies": {
    "vue-el": "*",
    "extend": "*",
    "vue": "~0.11.0-rc2",
    "superagent": "~0.18.1",
    "page": "~1.3.7",
    "bindall-standalone": "~1.0.5",
    "component-emitter": "~1.1.3"
  },
  "scripts": {
    "install": "napa"
  }
}
