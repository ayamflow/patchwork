    "fit": "./node_modules/fit/fit.min.js"
  },
  "browserify-shim": {
    "TweenMax": "TweenMax",
    "fit": "fit"
  },
  "devDependencies": {
    "gulp": "~3.8.1",
    "watchify": "~0.10.2",
    "gulp-plumber": "~0.6.3",
    "gulp-htmlbuild": "~0.2.0",
    "gulp-autoprefixer": "0.0.7",
    "gulp-sass": "~0.7.2",
    "vinyl-source-stream": "~0.1.1",
    "napa": "~0.4.1",
    "browserify": "~4.1.11",
    "browserify-shim": "~3.5.0",
    "gulp-cached": "0.0.3",
    "gulp-notify": "~1.3.1"
  },
  "dependencies": {
    "vue": "~0.10.5",
    "superagent": "~0.18.1",
    "page": "~1.3.7",
    "bindall-standalone": "~1.0.4"
  },
  "scripts": {
    "install": "napa"
  },
  "napa": {
    "greensock": "greensock/GreenSock-JS",
    "fit": "soulwire/fit.js"
  }
}
